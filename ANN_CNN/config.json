{
	"data": {
		"filename": "GBPUSD_Hourly_2018.csv",
		"columns": [
			"<CLOSE>",
			"<HIGH>",
			"<LOW>"
		],
		"sequence_length": 30,
		"train_test_split": 0.8,
		"normalise": true
	},
	"training": {
		"epochs": 20,
		"batch_size": 10
	},
	"model": {
		"loss": "mse",
		"optimizer": "adam",
		"save_dir": "saved_models"
	}
}